{
    "name": "behamin/bflow",
    "description": "a package that helps to implement user flow in laravel projects",
    "keywords": [
        "behamin",
        "user-flow",
        "user-state",
        "laravel",
        "flowchart",
        "redirect",
        "next"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alireza Bahram",
            "email": "alib327@gmail.com"
        },
        {
            "name": "Hamed Alizade",
            "email": "hamed.alizadeq@gamil.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Behamin\\BFlow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Behamin\\BFlow\\Tests\\" : "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BFlow\\BFlowServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.2|^7.3|^7.4|^8.0",
        "laravel/framework": "^7.0|^8.0",
        "ext-json": "*",
        "symfony/console": "^5.0"
    }
}
